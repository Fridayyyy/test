<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.example.file.mapper.UserMapper">

    <insert id="insertUser" parameterType="com.example.file.Model.User">
        insert into user (username,password,telephone)
        value (#{username},#{password},#{telephone})
    </insert>
    <select id="selectUser" resultType="com.example.file.Model.User">
        select * from user
    </select>
</mapper>